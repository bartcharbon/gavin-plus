<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
            <pattern>%d{yyyy-MM-dd_HH:mm:ss.SSS} %-5level- %msg%n</pattern>
		</encoder>
	</appender>
	<logger name="org.molgenis" level="INFO"/>
	<root level="INFO">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>